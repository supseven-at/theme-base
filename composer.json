{
	"name": "supseven/theme-base",
	"license": "GPL-2.0-or-later",
	"description": "Sitepackage Library of supseven. A swiss army knife for TYPO3 projects",
	"type": "typo3-cms-extension",
	"autoload": {
		"psr-4": {
			"Supseven\\ThemeBase\\": "Classes"
		}
	},
	"suggest": {
		"b13/bolt": "^2.3",
		"b13/doktypemapper": "^1.1.1",
		"eliashaeussler/typo3-warming": ">=1.1.0",
		"helhum/dotenv-connector": "^3",
		"ichhabrecht/content-defender": "^3.4",
		"josefglatz/hide-sys-template": "^4",
		"josefglatz/beuser-fastswitch": ">=5.0.2",
		"josefglatz/cropvariantsbuilder": ">=2.0.0",
		"studiomitte/live-search-extended": "*",
		"lolli/dbdoctor": "^0.5.0",
		"plan2net/webp": "^5.0.1",
		"supseven/supi": ">= 7",
		"christophlehmann/pagetreefilter": "^2",
		"spooner/deployer-information": "^1.4",
		"o-ba/custom_dashboard_widgets": "^1.3",
		"typo3/cms-linkvalidator": "^12.4",
		"typo3/cms-adminpanel": "^12.4"
	},
	"require-dev": {
		"typo3/cms-core": "^12.4",
		"typo3/cms-frontend": "^12.4",
		"typo3/cms-extbase": "^12.4",
		"symfony/expression-language": ">= 6",
		"typo3/cms-form": "^12.4",
		"enshrined/svg-sanitize": "^0.15",
		"typo3/cms-lowlevel": "^12.4",
		"phpunit/phpunit": "^10",
		"friendsofphp/php-cs-fixer": "^3.62",
		"typo3/testing-framework": "^8.2",
		"mikey179/vfsstream": "^1.6",
		"typo3fluid/fluid": ">= 2.10"
	},
	"autoload-dev": {
		"psr-4": {
			"Supseven\\ThemeBase\\Tests\\": "Tests/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "theme_base"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/cms-composer-installers": false,
			"typo3/class-alias-loader": false,
			"php-http/discovery": false
		}
	}
}
